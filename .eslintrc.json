{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "react-app",
    "react-app/jest"
  ],
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": "error",
    "react/prop-types": "off",
    "react-hooks/rules-of-hooks": "error",
    "react/jsx-uses-react": "error",
    "react-hooks/exhaustive-deps": "error"
  }
}
