  "jest": {
    "extensionsToTreatAsEsm": [
      ".jsx"
    ],
    "transform": {
      "\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!axios/)"
    ],
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ],
    "moduleNameMapper": {
      "@/(.*)": "./src/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/backend"
    ]
  },
  "typesVersions": {
    "<4.7": {
      "*": [
        "./src/index.jsx"
