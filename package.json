    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "customize-cra": "^1.0.0",
    "eslint": "^9.4.0",
