  "scripts": {
    "dev": "yarn run coc",
    "coc": "tsx --tsconfig ./jsconfig.json ./coc_boot.js",
    "start.be": "yarn --cwd ./backend run dev",
    "dev-p": "run-p dev dev.be",
    "new-dev": "webpack serve --mode development",
    "new-build": "webpack --mode development",
    "new-preview": "http-server ./new-build",
    "start": "cross-env PORT=3720 react-app-rewired start",
    "build": "cross-env PORT=3720 react-app-rewired build",
    "postinstall": "patch-package",
    "test": "vitest run",
    "test:old": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\"  react-app-rewired test --setupFilesAfterEnv ./src/setupTests.js",
    "test.update": "vitest run --update",
    "coverage": "vitest run --coverage",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "server": "json-server -p3725 --watch db.json",
    "server.person": "json-server -p3725 --watch db-person.json",
    "re": "yarn --cwd ./backend run echo",
    "lint": "eslint ./src",
    "cypress:open": "cypress open",
    "coverage.html": "http-server ./coverage -o",
    "test.e2e": "cypress run"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.7",
    "@babel/preset-env": "^7.24.6",
    "@react-dev-inspector/middleware": "^2.0.1",
    "@stylexjs/webpack-plugin": "^0.6.1",
    "@tsconfig/create-react-app": "^2.0.4",
