    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "react-app-rewired": "^2.2.1",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.4.5",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".jsx"
    ],
    "transform": {
      "\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!axios/)"
    ],
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ],
    "moduleNameMapper": {
      "@/(.*)": "./src/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/backend"
    ]
  },
  "typesVersions": {
    "<4.7": {
      "*": [
        "./src/index.jsx"
      ]
    }
  },
  "packageManager": "yarn@1.22.19",
